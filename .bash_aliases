# SK Commands
alias zsh-config='python ~/.config/zsh-config'
alias sk-symlink='cd ~/.config/zsh-config && make symlink'

# Personal Key
alias ssh-personal='sh ~/.config/zsh-config/scripts/change-ssh-key.sh personal'
alias ssh-credify='sh ~/.config/zsh-config/scripts/change-ssh-key.sh credify'

# Cheat.sh
alias cht.sh='sh ~/.config/zsh-config/scripts/cht.sh'

# Lazygit
alias lg='lazygit'
